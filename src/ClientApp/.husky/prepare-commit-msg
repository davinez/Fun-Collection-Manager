case "$OSTYPE" in
  solaris*) echo "SOLARIS" ;;
  darwin*)  echo "OSX" ;; 
  linux*)   echo "LINUX" ;;
  bsd*)     echo "BSD" ;;
  msys*)    echo "WINDOWS" ;;
  cygwin*)  echo "ALSO WINDOWS" ;;
  *)        echo "unknown: $OSTYPE" ;;
esac
echo "testdsfsgfdd"
if [[ "$OSTYPE" == "linux-gnu"* ]]; then
   echo "aaaaaaaaaaa"
elif [ "$OSTYPE" == "darwin"* ]; then
     echo "gfdgdfgdf"
elif [ "$OSTYPE" == "cygwin" ]; then
     echo "gfdgdfgdf"
elif [ "$OSTYPE" == "msys" ]; then
      echo "gfdgdfgdf"
elif [ "$OSTYPE" == "win32" ]; then
      echo "gfdgdfgdf"
elif [ "$OSTYPE" == "freebsd"* ]; then
      echo "gfdgdfgdf"
else
   echo "Unknown parameter"
fi

